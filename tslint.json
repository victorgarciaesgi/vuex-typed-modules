{
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "ordered-imports": false,
    "arrow-parens": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "no-console": false,
    "forin": false,
    "quotemark": false,
    "max-classes-per-file": false,
    "object-literal-key-quotes": false
  }
}
